{
  "name": "tree-sitter-slang",
  "version": "0.2.0",
  "description": "Slang shader language grammar for tree-sitter (based on tree-sitter-hls)",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "slang"
  ],
  "author": "Stephan Seitz",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "tree-sitter-c": "git://github.com/tree-sitter/tree-sitter-c.git",
    "tree-sitter-cli": "^0.20.8",
    "tree-sitter-cpp": "git://github.com/tree-sitter/tree-sitter-cpp.git",
    "tree-sitter-hlsl": "git://github.com/theHamsta/tree-sitter-hlsl.git"
  },
  "scripts": {
    "test": "tree-sitter test",
    "test-windows": "tree-sitter test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/theHamsta/tree-sitter-slang.git"
  },
  "tree-sitter": [
    {
      "scope": "source.cpp",
      "file-types": [
        "slang"
      ]
    }
  ]
}
